<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
    </ul>
  </div>          
  <div class="content">          
    <h2 class="title"><%= t("web-app-theme.all", :default => "All")  %> Events</h2>
    <div class="inner">
      <table class="table">
        <tr>             
          <th class="first">ID</th>
                    <th>
            <%= "Event Name" %>
          </th>
                    <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @events.each do |event| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td>
            <%= event.id %>
          </td>
          <td>
            <%= event.name %>
          </td>
                    <td>
            <%= event.created_at %>
          </td>
          <td class="last">
          </td>
        </tr>
        <tr>
          <td colspan="4">
            <div>
            <% event.process_videos.each do |video| -%>
               <div style="float:left;width:170px;">
                 <span> <%= video.start_time.strftime("%I:%M%p") %> <%= video.end_time.strftime(" to %I:%M%p") %></span>
                 <span>
                 <%= raw yt_client.my_video(get_watch_id(video.video_link)).embed_html5(:width=>150,:height=>150) %>
                 </span>
               </div>
            <% end %>
            </div>
          </td>
        </tr>          
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
